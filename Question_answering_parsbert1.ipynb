    "def loss_fn(out1,out2,target1,target2):\n",
    "  l1 = nn.BCEWithLogitsLoss()(out1, target1.float())\n",
    "  l2 = nn.BCEWithLogitsLoss()(out2, target2.float())\n",
    "  return l1 + l2"
   ]
